{
  "name": "flowmcp",
  "version": "2.1.2",
  "description": "Powerful toolkit for MCP (Model Context Protocol) tools and automated workflow processes",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/EgorKluch/flowmcp.git"
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test-dev": "NODE_OPTIONS=--experimental-vm-modules jest --watchAll",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cursor",
    "development-tools",
    "workflow",
    "processes",
    "scenarios",
    "automation",
    "toolkit",
    "typescript"
  ],
  "author": "Egor Kliuchantsev",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
