{
  "name": "mcp-manager",
  "version": "1.0.0",
  "description": "Development toolkit for creating and managing MCP (Model Context Protocol) servers",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test-dev": "NODE_OPTIONS=--experimental-vm-modules jest --watchAll",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cursor",
    "development-tools",
    "manager",
    "toolkit",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
